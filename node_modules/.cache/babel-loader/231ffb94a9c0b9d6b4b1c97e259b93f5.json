AAlB,EAAqB;AACjB,YAAIC,QAAQ,GAAGnD,KAAK,CAACG,KAAN,CAAYiD,MAAZ,CAAmB/B,IAAI,IAAIA,IAAI,CAACd,IAAL,CAAU8C,WAAV,GAAwBC,QAAxB,CAAiCL,IAAI,CAACI,WAAL,EAAjC,CAA3B,CAAf;AACAnB,QAAAA,MAAM,CAAC,UAAD,EAAaiB,QAAb,CAAN;AACH,OAHD,MAII;AACA,aAAKJ,QAAL,CAAc,cAAd;AACH;AACJ;;AArFI;AApDiB,CAAf,CAAf","sourcesContent":["import Vue from 'vue';\nimport Vuex from 'vuex';\nimport axios from \"axios\";\n\nVue.use(Vuex);\nexport default new Vuex.Store({\n    state: {\n        error: false,\n        loading: false,\n        Items: [],\n        Item: {\n            id: Number,\n            name: \"\",\n            order: Number,\n            complete: Boolean,\n            img: \"\"\n        },\n        favorites: [],\n    },\n    mutations: {\n        addToItems(state, payload) {\n            state.Items.push(payload);\n        },\n        setItems(state, payload) {\n            state.Items = payload\n        },\n        editItem(state, payload) {\n            Vue.set(state.Items, state.Items.findIndex(item => item.id == payload.id), payload);\n        },\n        clearFavorites(state) {\n            state.favorites = [];\n        },\n        removeItem(state, payload) {\n            state.Items.splice(state.Items.indexOf(payload), 1);\n        },\n        clearItems(state) {\n            state.Items = [];\n        },\n        revertError(state) {\n            state.error = false;\n        },\n        setError(state, payload){\n            state.error = payload;\n        },\n        getItem(state, payload){\n            state.Item = payload;\n        },\n        completeItem(state, payload){\n            Vue.set(state.Items, state.Items.findIndex(item => item.id == payload.id), payload);\n        },\n        startLoading(state) {\n            state.loading = true;\n        },\n        stopLoading(state){\n            state.loading = false;\n        }\n    },\n    actions: {\n        removeItem({commit}, payload){\n            axios.delete('http://localhost:3000/Items/' + payload.id)\n                .then(() => {\n                    commit(\"removeItem\", payload);\n                })\n                .catch((error) => {\n                    console.log(error);\n                });\n        },\n        completeItem({commit}, payload){\n            axios.patch('http://localhost:3000/Items/' + payload.id, {complete: true})\n                .then((response) =>{\n                    console.log(response.data);\n                    commit(\"completeItem\", response.data);\n                })\n                .catch((error) => {\n                    console.log(error);\n                });\n        },\n        clearFavorites({ commit }) {\n            commit(\"clearFavorites\");\n        },\n        revertError({commit}){\n            commit(\"revertError\");\n        },\n        //------------------Called from the form to add an item--------------\n        addToItems({commit}, payload) {\n            //start loading\n            commit(\"startLoading\");\n            axios.post('http://localhost:3000/Items', payload)\n                .then((response) => {\n                    commit(\"addToItems\", response.data);\n                    commit(\"setError\", false);\n                    commit(\"stopLoading\");\n                })\n                .catch(() => {\n                    commit(\"setError\", true);\n                    commit(\"stopLoading\");\n                });\n        },\n        editItem({commit}, payload){\n            axios.put('http://localhost:3000/Items/' + payload.id, payload)\n                .then((response) => {\n                    commit(\"editItem\", response.data);\n                })\n                .catch((error) => {\n                    console.log(error);\n                });\n        },\n        clearItems({commit}) {\n            commit(\"clearItems\");\n        },\n        setItems({commit}, payload) {\n            commit(\"setItems\", payload)\n        },\n        //---------- Called from the home page at initial set up-------\n        loadAllItems() {\n            axios.get('http://localhost:3000/Items')\n                .then((response) => {\n                    this.dispatch(\"setItems\", response.data);\n                })\n                .catch((error) => {\n                    console.log(error);\n                });\n        },\n        getItem({commit}, id){\n            axios.get('http://localhost:3000/Items/' + id)\n                .then((response) => {\n                    console.log(response.data);\n                    commit(\"getItem\", response.data)\n                })\n                .catch((error) => {\n                    console.log(error);\n                });\n        },\n        filterItems({commit, state}, text){\n            //if a string is not empty\n            if (text.length > 0) {\n                let newItems = state.Items.filter(item => item.name.toLowerCase().includes(text.toLowerCase()));\n                commit(\"setItems\", newItems);\n            }\n            else{\n                this.dispatch(\"loadAllItems\");\n            }\n        }\n    },\n});\n\n"]}]}