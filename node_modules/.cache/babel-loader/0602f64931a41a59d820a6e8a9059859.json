sm4 md3 v-for=\"pet in dogs\" :key=\"pet.breed\">\n        <app-dog item=\"pet\" @addToFavorites=\"addToFavorites\"></app-dog>\n      </v-flex>\n    </v-layout>\n  </v-container>\n</template>\n<script>\nimport Dog from '../components/item.vue';\nimport {Dogs} from \"../data/dogs\";\nimport axios from 'axios';\n//base URL that we'll be calling\naxios.defaults.baseURL = 'https://dog.ceo/api';\nimport { mapActions } from 'vuex';\n\nexport default {\n  components: {\n    appDog: Dog,\n  },\n  data() {\n    return {\n      dogs: Dogs\n    };\n  },\n  created() {\n    const linksArray = this.dogs.map(\n        dog => \"/breed/\" + dog.breed + \"/images/random\"\n    );\n    axios.all(linksArray.map(link => axios.get(link)))\n        .then(\n            axios.spread((...res) => {\n              this.dogs.forEach((dog, index) => {\n                dog.img = res[index].data.message;\n              });\n            })\n        );\n  },\n  methods: {\n    ...mapActions([\"addToFavorites\"])\n  }\n\n};\n</script>\n\n<style scoped>\np {\n  margin: 0;\n}\n</style>"],"sourceRoot":"src/views"}]}